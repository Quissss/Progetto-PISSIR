@page
@model Progetto.App.Pages.DashMwBotModel
@{
    ViewData["Title"] = "Dashboard MwBots";
}

<h2>@ViewData["Title"]</h2>



<div class="container-fluid">
    <div class="row">
        <!-- Sidebar -->
        <nav id="sidebar" class="col-md-3 col-lg-2 d-md-block bg-dark sidebar collapse">
            <div class="position-sticky">
                <ul class="nav flex-column">
                    <li class="nav-item">
                        <a class="nav-link active text-white" href="#">
                            <i class="bi bi-house-door-fill"></i>
                            Dashboard
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="/DashMwBot">
                            MwBots
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="/Parking">
                            Parking
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="/DashParking">
                            ParkingSlot
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="/Recharge">
                            Recharge
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="#">
                            Integrations
                        </a>
                    </li>
                </ul>
                <h6 class="sidebar-heading d-flex justify-content-between align-items-center px-3 mt-4 mb-1 text-muted">
                    <span>SAVED REPORTS</span>
                </h6>
                <ul class="nav flex-column mb-2">
                    <li class="nav-item">
                        <a class="nav-link text-white" href="#">
                            Current month
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="#">
                            Last quarter
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="#">
                            Social engagement
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="#">
                            Year-end sale
                        </a>
                    </li>
                </ul>
                <ul class="nav flex-column mb-2">
                    <li class="nav-item">
                        <a class="nav-link text-white" href="#">
                            Settings
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="#">
                            Sign out
                        </a>
                    </li>
                </ul>
            </div>
        </nav>

        <!-- Main content -->
        <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
            <div id="mwBotGrid"></div>

            <!-- Line chart -->
            <canvas class="my-4 w-100" id="myChart" width="900" height="380"></canvas>


    </div>
    </main>
</div>
</div>




@section Scripts {
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jsgrid/1.5.3/jsgrid.min.js"></script>
    <script type="text/javascript" src="~/js/jsGridMwBot.js"></script>
}